<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>
    <h1> <%= question %></h1>

    <ol>
        <% for(const key in option) { %>

            <li><%= option[key].optionStatement %>  has got <%= option[key].count %> votes.</li>
            
        <% } %>     
    </ol>

    <form method="POST">
        <button type="submit"> Delete this poll </button>
    </form>
</body>
</html>